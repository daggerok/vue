{
  "name": "vue-js",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf -rf dist",
    "build": "npm-run-all build:*",
    "build:copy-favicon": "copy favicon.ico dist/",
    "build:copy-vendors": "copy node_modules/{normalize.css/normalize.css,vue/dist/vue.js} dist/",
    "build:html": "html-minifier index.html --html5 --remove-script-type-attributes --remove-style-link-type-attributes --collapse-whitespace --remove-comments -o dist/index.html",
    "build:js-vendors": "uglifyjs node_modules/vue/dist/vue.min.js --output dist/node_modules/vue/dist/vue.js",
    "build:js": "uglifyjs app.js --output dist/app.js --source-map dist/app.js.map --compress --mangle --support-ie8",
    "build:css-normalize": "cssmin node_modules/normalize.css/normalize.css > dist/node_modules/normalize.css/normalize.css",
    "build:css-app": "cssmin app.css > dist/app.css",
    "start": "lite-server"
  },
  "keywords": [
    "vue.js"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "normalize.css": "^5.0.0",
    "vue": "2.1.8"
  },
  "devDependencies": {
    "copy": "0.3.0",
    "cssmin": "^0.4.3",
    "html-minifier": "^3.2.3",
    "lite-server": "2.2.2",
    "ncp": "2.0.0",
    "npm-run-all": "4.0.0",
    "rimraf": "2.5.4",
    "uglify-js": "^2.7.5"
  }
}
